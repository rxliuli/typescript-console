{
  "name": "typescript-console",
  "description": "Run and debug TypeScript code in the Chrome DevTools.",
  "private": true,
  "type": "module",
  "version": "0.2.0",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
