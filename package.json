    "@types/babel__traverse": "^7.20.6",
    "@types/lodash-es": "^4.17.12",
    "@wxt-dev/module-svelte": "^1.0.0",
    "autoprefixer": "^10.4.20",
    "monaco-editor": "^0.51.0",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.4",
    "tailwindcss": "^3.4.9",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "wxt": "^0.19.1"
  },
